{
  "name": "@bavaan/admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "populate": "ts-node populate.ts",
    "compile-admin-ui": "ts-node src/compile-admin-ui.ts",
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "start": "concurrently yarn:run:*",
    "build": "rimraf dist && rimraf admin-ui && tsc && tsc -p tsconfig.migrations.json && yarn compile-admin-ui",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "e2e:reviews": "jest --config src/plugins/reviews/e2e/config/jest-config.js"
  },
  "dependencies": {
    "@bavaan/vendure-braintree-payment-plugin": "^0.0.1",
    "@bavaan/vendure-domestic-shipping-plugin": "^0.0.1",
    "@bavaan/vendure-international-shipping-plugin": "^0.0.1",
    "@bavaan/vendure-amount-promotion-plugin": "^0.0.1",
    "@bavaan/vendure-shipping-promotion-plugin": "^0.0.1",
    "@bavaan/vendure-order-vendor-plugin": "^0.0.1",
    "@bavaan/vendure-product-management": "^0.0.1",
    "@bavaan/vendure-product-recommendations": "^0.0.1",
    "@bavaan/vendure-product-recently-viewed-plugin": "^0.0.1",
    "@bavaan/vendure-reviews-plugin": "^0.0.1",
    "@bavaan/vendure-tier-price-plugin": "^0.0.1",
    "@bavaan/vendure-contact-plugin": "^0.0.1",
    "@landofcoder/vendure-payumoney-payment-plugin": "^0.0.1",
    "@bavaan/vendure-paypal-payment-plugin": "^0.0.1",
    "@bavaan/vendure-delhivery-plugin": "^0.0.1",
    "@bavaan/vendure-order-fullfill-plugin": "^0.0.1",
    "@bavaan/vendure-cod-payment-plugin": "^0.0.1",
    "@bavaan/vendure-ecomexpress-plugin": "^0.0.1",
    "@vendure/admin-ui-plugin":  "0.17.1",
    "@bavaan/ui-devkit": "0.17.1",
    "@vendure/asset-server-plugin": "0.17.1",
    "@vendure/core": "0.17.1",
    "@vendure/email-plugin":  "0.17.1",
    "mysql": "^2.18.1",
    "ngx-daterangepicker-material": "v2.3.x",
    "pg": "^8.2.1",
    "sqlite3": "4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-compatibility": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@types/braintree": "^2.22.4",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vendure/create": "0.17.1",
    "@vendure/testing": "0.17.1",
    "concurrently": "5.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "resolutions": {
    "tslib": "1.11.2"
  }
}
