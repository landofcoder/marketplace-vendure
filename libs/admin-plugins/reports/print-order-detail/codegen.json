{
  "overwrite": true,
  "config": {
    "strict": true
  },
  "generates": {
    "src/generated-admin-types.ts": {
      "schema": "http://localhost:3000/admin-api",
      "plugins": [
        {
          "add": "// tslint:disable"
        },
        "typescript",
        "typescript-compatibility"
      ]
    },
    "src/ui/generated-types.ts": {
      "schema": "http://localhost:3000/admin-api",
      "documents": "src/ui/**/*.graphql.ts",
      "plugins": [
        {
          "add": "// tslint:disable"
        },
        "typescript",
        "typescript-compatibility",
        "typescript-operations"
      ]
    }
  }
}
